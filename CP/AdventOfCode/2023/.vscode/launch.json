{
    "version": "0.2.0",
    "configurations": [
        

        {
            "name": "Debug C Program",
            "type": "cppdbg",
            "request": "launch",
            // The path to the executable file generated by your build command.
            "program": "${workspaceFolder}/day17",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            // This tells VS Code to run the "build_c_debug" task before launching the debugger.
            "preLaunchTask": "build_c_debug"
        }
    ]
}